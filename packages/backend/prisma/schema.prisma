generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Organization {
  id                   String                @id @default(uuid())
  name                 String
  slug                 String                @unique
  address              String?
  phone                String?
  email                String?
  website              String?
  description          String?
  isActive             Boolean               @default(true)
  createdAt            DateTime              @default(now())
  updatedAt            DateTime              @updatedAt
  analysisTemplates    AnalysisTemplate[]
  appointments         Appointment[]
  departments          Department[]
  employees            Employee[]
  invoices             Invoice[]
  parameterDefinitions ParameterDefinition[]
  allergies            PatientAllergy[]
  patientParameters    PatientParameter[]
  patients             Patient[]
  payments             Payment[]
  protocolTemplates    ProtocolTemplate[]
  roles                Role[]
  serviceOrders        ServiceOrder[]
  serviceTypes         ServiceType[]
  services             Service[]
  titles               Title[]
  users                User[]
  visits               Visit[]

  @@map("organizations")
}

model User {
  id                    String                @id @default(uuid())
  phone                 String                @unique
  password              String
  role                  UserRole
  isActive              Boolean               @default(true)
  lastLoginAt           DateTime?
  createdAt             DateTime              @default(now())
  updatedAt             DateTime              @updatedAt
  organizationId        String?
  canceledAppointments  Appointment[]         @relation("AppointmentCanceledBy")
  confirmedAppointments Appointment[]         @relation("AppointmentConfirmedBy")
  createdAppointments   Appointment[]         @relation("AppointmentCreatedBy")
  employee              Employee?
  performedOrders       ServiceOrder[]        @relation("PerformedBy")
  roleAssignments       UserRole_Assignment[]
  organization          Organization?         @relation(fields: [organizationId], references: [id])

  @@map("users")
}

model Patient {
  id                  String             @id @default(uuid())
  patientId           String?
  firstName           String
  middleName          String?
  lastName            String
  dateOfBirth         DateTime           @db.Date
  gender              Gender
  status              PatientStatus      @default(ACTIVE)
  lastVisitedAt       DateTime?
  passportSeries      String?
  passportNumber      String?
  passportIssuedBy    String?
  passportIssueDate   DateTime?          @db.Date
  passportExpiryDate  DateTime?          @db.Date
  primaryLanguageId   String?
  secondaryLanguageId String?
  countryId           String?
  regionId            String?
  cityId              String?
  districtId          String?
  address             String?
  createdAt           DateTime           @default(now())
  updatedAt           DateTime           @updatedAt
  organizationId      String
  appointments        Appointment[]
  invoices            Invoice[]
  allergies           PatientAllergy[]
  contacts            PatientContact[]
  doctors             PatientDoctor[]
  patientParameters   PatientParameter[]
  city                Location?          @relation("PatientCity", fields: [cityId], references: [id])
  country             Location?          @relation("PatientCountry", fields: [countryId], references: [id])
  district            Location?          @relation("PatientDistrict", fields: [districtId], references: [id])
  organization        Organization       @relation(fields: [organizationId], references: [id])
  primaryLanguage     Language?          @relation("PatientPrimaryLanguage", fields: [primaryLanguageId], references: [id])
  region              Location?          @relation("PatientRegion", fields: [regionId], references: [id])
  secondaryLanguage   Language?          @relation("PatientSecondaryLanguage", fields: [secondaryLanguageId], references: [id])
  serviceOrders       ServiceOrder[]
  visits              Visit[]

  @@unique([patientId, organizationId])
  @@map("patients")
}

model PatientContact {
  id                        String              @id @default(uuid())
  relation                  ContactRelationType
  type                      ContactType
  firstName                 String?
  lastName                  String?
  primaryPhone              String
  secondaryPhone            String?
  address                   String?
  city                      String?
  state                     String?
  country                   String?
  textNotificationsEnabled  Boolean             @default(false)
  emailNotificationsEnabled Boolean             @default(false)
  createdAt                 DateTime            @default(now())
  updatedAt                 DateTime            @updatedAt
  patientId                 String
  patient                   Patient             @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@map("patient_contacts")
}

model PatientDoctor {
  id         String   @id @default(uuid())
  patientId  String
  employeeId String
  assignedAt DateTime @default(now())
  isActive   Boolean  @default(true)
  employee   Employee @relation(fields: [employeeId], references: [id], onDelete: Cascade)
  patient    Patient  @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@unique([patientId, employeeId])
  @@map("patient_doctors")
}

model Department {
  id             String         @id @default(uuid())
  name           String
  code           String?
  description    String?
  headId         String?
  isActive       Boolean        @default(true)
  order          Int?
  createdAt      DateTime       @default(now())
  updatedAt      DateTime       @updatedAt
  organizationId String
  head           Employee?      @relation("DepartmentHead", fields: [headId], references: [id])
  organization   Organization   @relation(fields: [organizationId], references: [id])
  staff          Employee[]     @relation("DepartmentStaff")
  serviceOrders  ServiceOrder[]
  services       Service[]

  @@unique([organizationId, name])
  @@unique([organizationId, code])
  @@map("departments")
}

model Service {
  id             String          @id @default(uuid())
  code           String
  name           String
  description    String?
  type           ServiceTypeEnum
  price          Decimal?        @db.Decimal(12, 2)
  durationMin    Int?
  isActive       Boolean         @default(true)
  createdAt      DateTime        @default(now())
  updatedAt      DateTime        @updatedAt
  organizationId String
  departmentId   String?
  employeeId     String?
  appointments   Appointment[]
  invoiceItems   InvoiceItem[]
  serviceOrders  ServiceOrder[]
  department     Department?     @relation(fields: [departmentId], references: [id])
  employee       Employee?       @relation(fields: [employeeId], references: [id])
  organization   Organization    @relation(fields: [organizationId], references: [id])

  @@unique([code, organizationId])
  @@map("services")
}

model Appointment {
  id             String            @id @default(uuid())
  scheduledAt    DateTime
  duration       Int
  status         AppointmentStatus @default(SCHEDULED)
  notes          String?
  reason         String?
  roomNumber     String?
  confirmedById  String?
  confirmedAt    DateTime?
  canceledById   String?
  canceledAt     DateTime?
  cancelReason   String?
  checkInAt      DateTime?
  checkOutAt     DateTime?
  createdAt      DateTime          @default(now())
  updatedAt      DateTime          @updatedAt
  organizationId String
  patientId      String
  employeeId     String
  serviceId      String?
  createdById    String
  canceledBy     User?             @relation("AppointmentCanceledBy", fields: [canceledById], references: [id])
  confirmedBy    User?             @relation("AppointmentConfirmedBy", fields: [confirmedById], references: [id])
  createdBy      User              @relation("AppointmentCreatedBy", fields: [createdById], references: [id])
  employee       Employee          @relation(fields: [employeeId], references: [id])
  organization   Organization      @relation(fields: [organizationId], references: [id])
  patient        Patient           @relation(fields: [patientId], references: [id])
  service        Service?          @relation(fields: [serviceId], references: [id])
  visits         Visit[]

  @@map("appointments")
}

model Visit {
  id                 String             @id @default(uuid())
  visitDate          DateTime           @default(now())
  status             VisitStatus        @default(WAITING)
  type               VisitType          @default(STANDARD)
  notes              String?
  queuedAt           DateTime?
  startedAt          DateTime?
  completedAt        DateTime?
  waitingTimeMinutes Int?
  serviceTimeMinutes Int?
  createdAt          DateTime           @default(now())
  updatedAt          DateTime           @updatedAt
  organizationId     String
  appointmentId      String?
  patientId          String
  employeeId         String
  protocolId         String?
  protocolData       String?
  invoices           Invoice[]
  allergies          PatientAllergy[]
  patientParameters  PatientParameter[]
  prescriptions      Prescription[]
  serviceOrders      ServiceOrder[]
  appointment        Appointment?       @relation(fields: [appointmentId], references: [id])
  employee           Employee           @relation("VisitDoctor", fields: [employeeId], references: [id])
  organization       Organization       @relation(fields: [organizationId], references: [id])
  patient            Patient            @relation(fields: [patientId], references: [id])
  protocol           ProtocolTemplate?  @relation(fields: [protocolId], references: [id])

  @@index([organizationId, employeeId, visitDate])
  @@index([organizationId, status])
  @@index([employeeId, status, queuedAt])
  @@map("visits")
}


model Prescription {
  id          String   @id @default(uuid())
  name        String
  dosage      String?
  frequency   String?
  duration    String?
  notes       String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  visitId     String
  createdById String
  createdBy   Employee @relation(fields: [createdById], references: [id])
  visit       Visit    @relation(fields: [visitId], references: [id], onDelete: Cascade)

  @@map("prescriptions")
}

model Employee {
  id                       String                @id @default(uuid())
  userId                   String?               @unique
  employeeId               String?
  firstName                String
  middleName               String?
  lastName                 String
  dateOfBirth              DateTime?             @db.Date
  gender                   Gender?
  email                    String?
  phone                    String?
  secondaryPhone           String?
  workPhone                String?
  passportSeries           String?
  passportNumber           String?
  passportIssuedBy         String?
  passportIssueDate        DateTime?             @db.Date
  passportExpiryDate       DateTime?             @db.Date
  titleId                  String?
  salary                   Decimal?              @db.Decimal(12, 2)
  hireDate                 DateTime?             @db.Date
  terminationDate          DateTime?             @db.Date
  status                   EmployeeStatus        @default(ACTIVE)
  workSchedule             Json?
  primaryLanguageId        String?
  secondaryLanguageId      String?
  countryId                String?
  regionId                 String?
  cityId                   String?
  districtId               String?
  address                  String?
  textNotificationsEnabled Boolean               @default(false)
  notes                    String?
  createdAt                DateTime              @default(now())
  updatedAt                DateTime              @updatedAt
  organizationId           String
  departmentId             String?
  appointments             Appointment[]
  departmentsAsHead        Department[]          @relation("DepartmentHead")
  serviceTypes             EmployeeServiceType[]
  city                     Location?             @relation("EmployeeCity", fields: [cityId], references: [id])
  country                  Location?             @relation("EmployeeCountry", fields: [countryId], references: [id])
  department               Department?           @relation("DepartmentStaff", fields: [departmentId], references: [id])
  district                 Location?             @relation("EmployeeDistrict", fields: [districtId], references: [id])
  organization             Organization          @relation(fields: [organizationId], references: [id])
  primaryLanguage          Language?             @relation("EmployeePrimaryLanguage", fields: [primaryLanguageId], references: [id])
  region                   Location?             @relation("EmployeeRegion", fields: [regionId], references: [id])
  secondaryLanguage        Language?             @relation("EmployeeSecondaryLanguage", fields: [secondaryLanguageId], references: [id])
  title                    Title?                @relation(fields: [titleId], references: [id])
  user                     User?                 @relation(fields: [userId], references: [id])
  deletedFiles             File[]                @relation("FileDeletedBy")
  uploadedFiles            File[]                @relation("FileUploadedBy")
  createdInvoices          Invoice[]             @relation("EmployeeInvoices")
  recordedAllergies        PatientAllergy[]
  patientDoctors           PatientDoctor[]
  recordedParameters       PatientParameter[]
  processedPayments        Payment[]             @relation("EmployeePayments")
  prescriptions            Prescription[]
  doctorOrders             ServiceOrder[]        @relation("DoctorOrders")
  services                 Service[]
  visits                   Visit[]               @relation("VisitDoctor")

  @@unique([employeeId, organizationId])
  @@map("employees")
}

model Role {
  id              String                @id @default(uuid())
  name            String
  description     String?
  isActive        Boolean               @default(true)
  isSystem        Boolean               @default(false)
  createdAt       DateTime              @default(now())
  updatedAt       DateTime              @updatedAt
  organizationId  String
  permissions     RolePermission[]
  organization    Organization          @relation(fields: [organizationId], references: [id])
  userAssignments UserRole_Assignment[]

  @@unique([name, organizationId])
  @@map("roles")
}

model Permission {
  id              String           @id @default(uuid())
  name            String           @unique
  resource        String
  action          PermissionAction
  description     String?
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
  rolePermissions RolePermission[]

  @@unique([resource, action])
  @@map("permissions")
}

model RolePermission {
  id           String     @id @default(uuid())
  roleId       String
  permissionId String
  permission   Permission @relation(fields: [permissionId], references: [id], onDelete: Cascade)
  role         Role       @relation(fields: [roleId], references: [id], onDelete: Cascade)

  @@unique([roleId, permissionId])
  @@map("role_permissions")
}

model UserRole_Assignment {
  id         String    @id @default(uuid())
  userId     String
  roleId     String
  assignedAt DateTime  @default(now())
  assignedBy String?
  expiresAt  DateTime?
  role       Role      @relation(fields: [roleId], references: [id], onDelete: Cascade)
  user       User      @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, roleId])
  @@map("user_role_assignments")
}

model Title {
  id             String       @id @default(uuid())
  name           String
  description    String?
  isActive       Boolean      @default(true)
  createdAt      DateTime     @default(now())
  updatedAt      DateTime     @updatedAt
  organizationId String
  employees      Employee[]
  organization   Organization @relation(fields: [organizationId], references: [id])

  @@unique([name, organizationId])
  @@map("titles")
}

model ServiceType {
  id             String                @id @default(uuid())
  name           String
  code           String?
  description    String?
  isActive       Boolean               @default(true)
  createdAt      DateTime              @default(now())
  updatedAt      DateTime              @updatedAt
  organizationId String
  employees      EmployeeServiceType[]
  organization   Organization          @relation(fields: [organizationId], references: [id])

  @@unique([name, organizationId])
  @@unique([code, organizationId])
  @@map("service_types")
}

model EmployeeServiceType {
  id            String      @id @default(uuid())
  employeeId    String
  serviceTypeId String
  employee      Employee    @relation(fields: [employeeId], references: [id], onDelete: Cascade)
  serviceType   ServiceType @relation(fields: [serviceTypeId], references: [id], onDelete: Cascade)

  @@unique([employeeId, serviceTypeId])
  @@map("employee_service_types")
}

model Location {
  id                  String       @id @default(uuid())
  name                String
  code                String?
  type                LocationType
  weight              Int          @default(0)
  description         String?
  searchField         String?
  isActive            Boolean      @default(true)
  createdAt           DateTime     @default(now())
  updatedAt           DateTime     @updatedAt
  parentId            String?
  employeesAsCity     Employee[]   @relation("EmployeeCity")
  employeesAsCountry  Employee[]   @relation("EmployeeCountry")
  employeesAsDistrict Employee[]   @relation("EmployeeDistrict")
  employeesAsRegion   Employee[]   @relation("EmployeeRegion")
  parent              Location?    @relation("LocationHierarchy", fields: [parentId], references: [id])
  children            Location[]   @relation("LocationHierarchy")
  patientsAsCity      Patient[]    @relation("PatientCity")
  patientsAsCountry   Patient[]    @relation("PatientCountry")
  patientsAsDistrict  Patient[]    @relation("PatientDistrict")
  patientsAsRegion    Patient[]    @relation("PatientRegion")

  @@index([parentId])
  @@index([type])
  @@index([weight])
  @@index([name])
  @@index([code])
  @@index([searchField])
  @@map("locations")
}

model Language {
  id                 String     @id @default(uuid())
  name               String     @unique
  code               String?    @unique
  nativeName         String?
  weight             Int        @default(0)
  description        String?
  isActive           Boolean    @default(true)
  createdAt          DateTime   @default(now())
  updatedAt          DateTime   @updatedAt
  primaryEmployees   Employee[] @relation("EmployeePrimaryLanguage")
  secondaryEmployees Employee[] @relation("EmployeeSecondaryLanguage")
  primaryPatients    Patient[]  @relation("PatientPrimaryLanguage")
  secondaryPatients  Patient[]  @relation("PatientSecondaryLanguage")

  @@index([weight])
  @@index([name])
  @@map("languages")
}

model ProtocolTemplate {
  id             String         @id @default(uuid())
  name           String
  description    String
  content        String
  templateType   String         @default("richtext")
  isActive       Boolean        @default(true)
  createdAt      DateTime       @default(now())
  updatedAt      DateTime       @updatedAt
  createdBy      String?
  updatedBy      String?
  organizationId String
  organization   Organization   @relation(fields: [organizationId], references: [id])
  serviceOrders  ServiceOrder[]
  visits         Visit[]

  @@map("protocol_templates")
}

model AnalysisTemplate {
  id             String       @id @default(uuid())
  name           String
  code           String
  description    String?
  content        String
  isActive       Boolean      @default(true)
  createdAt      DateTime     @default(now())
  updatedAt      DateTime     @updatedAt
  createdBy      String?
  updatedBy      String?
  organizationId String
  organization   Organization @relation(fields: [organizationId], references: [id])

  @@unique([code, organizationId])
  @@index([organizationId])
  @@index([name])
  @@map("analysis_templates")
}

/// ===============================
/// SERVICE ORDER (назначения врача)
/// ===============================
model ServiceOrder {
  id                 String             @id @default(uuid())
  patientId          String
  visitId            String?
  doctorId           String
  serviceId          String
  departmentId       String?
  protocolTemplateId String?
  status             OrderStatus        @default(ORDERED)
  paymentStatus      PaymentStatus      @default(UNPAID)
  queueNumber        Int?
  queueStatus        QueueStatus?
  queuedAt           DateTime?
  startedAt          DateTime?
  finishedAt         DateTime?
  resultText         String?
  resultData         Json?
  resultFileUrl      String?
  resultAt           DateTime?
  performedById      String?
  organizationId     String
  createdAt          DateTime           @default(now())
  updatedAt          DateTime           @updatedAt
  invoiceItem        InvoiceItem?
  patientParameters  PatientParameter[]
  department         Department?        @relation(fields: [departmentId], references: [id])
  doctor             Employee           @relation("DoctorOrders", fields: [doctorId], references: [id])
  organization       Organization       @relation(fields: [organizationId], references: [id])
  patient            Patient            @relation(fields: [patientId], references: [id])
  performedBy        User?              @relation("PerformedBy", fields: [performedById], references: [id])
  protocolTemplate   ProtocolTemplate?  @relation(fields: [protocolTemplateId], references: [id])
  service            Service            @relation(fields: [serviceId], references: [id])
  visit              Visit?             @relation(fields: [visitId], references: [id])

  @@index([patientId])
  @@index([visitId])
  @@index([doctorId])
  @@index([serviceId])
  @@index([departmentId])
  @@index([status])
  @@index([paymentStatus])
  @@index([queueStatus])
  @@index([departmentId, queueStatus, queueNumber])
  @@index([departmentId, queuedAt])
  @@index([organizationId])
  @@map("service_orders")
}

model ParameterDefinition {
  id             String        @id @default(uuid())
  code           String
  name           String
  category       String
  valueType      ParameterType
  defaultUnit    String?
  normalRange    Json?
  description    String?
  isActive       Boolean       @default(true)
  organizationId String
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt
  organization   Organization  @relation(fields: [organizationId], references: [id])

  @@unique([code, organizationId])
  @@index([organizationId])
  @@map("parameter_definitions")
}

model PatientParameter {
  id             String          @id @default(uuid())
  patientId      String
  visitId        String?
  serviceOrderId String?
  parameterCode  String
  valueNumeric   Decimal?        @db.Decimal(12, 4)
  valueText      String?
  valueBoolean   Boolean?
  valueJson      Json?
  unit           String?
  measuredAt     DateTime        @default(now())
  recordedById   String
  source         ParameterSource @default(MANUAL)
  organizationId String
  createdAt      DateTime        @default(now())
  updatedAt      DateTime        @updatedAt
  organization   Organization    @relation(fields: [organizationId], references: [id])
  patient        Patient         @relation(fields: [patientId], references: [id], onDelete: Cascade)
  recordedBy     Employee        @relation(fields: [recordedById], references: [id])
  serviceOrder   ServiceOrder?   @relation(fields: [serviceOrderId], references: [id])
  visit          Visit?          @relation(fields: [visitId], references: [id], onDelete: Cascade)

  @@index([patientId, parameterCode, measuredAt])
  @@index([visitId])
  @@index([serviceOrderId])
  @@index([organizationId])
  @@map("patient_parameters")
}

model PatientAllergy {
  id             String           @id @default(uuid())
  patientId      String
  visitId        String?
  recordedById   String
  substance      String
  reaction       String?
  severity       AllergySeverity?
  note           String?
  organizationId String
  createdAt      DateTime         @default(now())
  updatedAt      DateTime         @updatedAt
  organization   Organization     @relation(fields: [organizationId], references: [id])
  patient        Patient          @relation(fields: [patientId], references: [id], onDelete: Cascade)
  recordedBy     Employee         @relation(fields: [recordedById], references: [id])
  visit          Visit?           @relation(fields: [visitId], references: [id], onDelete: Cascade)

  @@index([patientId])
  @@index([visitId])
  @@index([organizationId])
  @@map("patient_allergies")
}

model Invoice {
  id             String        @id @default(uuid())
  invoiceNumber  String
  patientId      String
  visitId        String?
  totalAmount    Decimal       @db.Decimal(12, 2)
  paidAmount     Decimal       @default(0) @db.Decimal(12, 2)
  status         PaymentStatus @default(UNPAID)
  notes          String?
  dueDate        DateTime?     @db.Date
  createdById    String
  organizationId String
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt
  items          InvoiceItem[]
  createdBy      Employee      @relation("EmployeeInvoices", fields: [createdById], references: [id])
  organization   Organization  @relation(fields: [organizationId], references: [id])
  patient        Patient       @relation(fields: [patientId], references: [id])
  visit          Visit?        @relation(fields: [visitId], references: [id])
  payments       Payment[]

  @@unique([invoiceNumber, organizationId])
  @@index([patientId])
  @@index([visitId])
  @@index([status])
  @@index([organizationId])
  @@index([createdAt])
  @@map("invoices")
}

model InvoiceItem {
  id             String        @id @default(uuid())
  invoiceId      String
  serviceId      String
  serviceOrderId String?       @unique
  description    String?
  quantity       Int           @default(1)
  unitPrice      Decimal       @db.Decimal(12, 2)
  totalPrice     Decimal       @db.Decimal(12, 2)
  discount       Decimal?      @default(0) @db.Decimal(12, 2)
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt
  invoice        Invoice       @relation(fields: [invoiceId], references: [id], onDelete: Cascade)
  service        Service       @relation(fields: [serviceId], references: [id])
  serviceOrder   ServiceOrder? @relation(fields: [serviceOrderId], references: [id])

  @@index([invoiceId])
  @@index([serviceId])
  @@index([serviceOrderId])
  @@map("invoice_items")
}

model Payment {
  id             String        @id @default(uuid())
  invoiceId      String
  amount         Decimal       @db.Decimal(12, 2)
  paymentMethod  PaymentMethod
  transactionId  String?
  notes          String?
  paidById       String
  organizationId String
  paidAt         DateTime      @default(now())
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt
  invoice        Invoice       @relation(fields: [invoiceId], references: [id])
  organization   Organization  @relation(fields: [organizationId], references: [id])
  paidBy         Employee      @relation("EmployeePayments", fields: [paidById], references: [id])

  @@index([invoiceId])
  @@index([organizationId])
  @@index([paidAt])
  @@map("payments")
}

model File {
  id           String          @id @default(uuid())
  filename     String
  storedName   String          @unique
  path         String
  mimeType     String
  size         Int
  title        String?
  description  String?
  category     FileCategory    @default(GENERAL)
  width        Int?
  height       Int?
  uploadedById String
  uploadedAt   DateTime        @default(now())
  entityType   FileEntityType?
  entityId     String?
  deletedAt    DateTime?
  deletedById  String?
  deletedBy    Employee?       @relation("FileDeletedBy", fields: [deletedById], references: [id])
  uploadedBy   Employee        @relation("FileUploadedBy", fields: [uploadedById], references: [id])

  @@index([entityType, entityId])
  @@index([category])
  @@index([uploadedById])
  @@index([uploadedAt])
  @@map("files")
}

enum UserRole {
  SUPER_ADMIN
  ADMIN
  DOCTOR
  NURSE
  RECEPTIONIST
  PATIENT
}

enum EmployeeStatus {
  ACTIVE
  INACTIVE
  ON_LEAVE
  TERMINATED
}

enum PermissionAction {
  CREATE
  READ
  UPDATE
  DELETE
  MANAGE
}

enum ServiceTypeEnum {
  CONSULTATION
  LAB
  DIAGNOSTIC
  PROCEDURE
  OTHER
}

enum AppointmentStatus {
  SCHEDULED
  CONFIRMED
  IN_QUEUE
  IN_PROGRESS
  COMPLETED
  CANCELLED
  NO_SHOW
}

enum VisitType {
  STANDARD
  WITHOUT_QUEUE
  EMERGENCY
}

enum VisitStatus {
  WAITING
  IN_PROGRESS
  COMPLETED
  CANCELED
}

enum RecordType {
  COMPLAINTS
  EXAMINATION
  DIAGNOSIS
  PRESCRIPTION
  NOTE
}

enum Gender {
  MALE
  FEMALE
